{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/code/lasagnas.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "run",
      "command": "dotnet",
      "type": "process",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/code/lasagnas.csproj",
        "${workspaceFolder}/data"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
